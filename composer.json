{
  "name": "tinywan/think-jwt",
  "description": "JSON Web Token (JWT) for Think plugin",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.1||^8.0",
    "ext-json": "*",
    "firebase/php-jwt": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "tinywan\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tinywan\\tests\\": "tests/"
    }
  },
  "extra": {
    "think": {
      "services": [
        "tinywan\\service\\JWTService"
      ]
    }
  },
  "require-dev": {
    "topthink/framework": "^6.0"
  }
}
